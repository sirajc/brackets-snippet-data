[
  {
    "name": "AngularJS module",
    "trigger": "ngmodc",
    "usage": "ngc moduleName",
    "description": "Create AngularJS module",
    "template": "angular.module('$${module}', [!!{cursor}]);"
  },
  {
    "name": "Get AngularJS module",
    "trigger": "ngmod",
    "usage": "ng moduleName",
    "description": "Getter of AngularJS module",
    "template": "angular.module('$${module}')\n!!{cursor}"
  },
  {
    "name": "Controller",
    "trigger": "ngcon",
    "usage": "ng controllerName",
    "description": "AngularJS controller",
    "template": ".controller('$${controller}', $${controller});!!{cursor}"
  },
  {
    "name": "Service",
    "trigger": "ngser",
    "usage": "ng serviceName",
    "description": "AngularJS service",
    "template": ".service('$${service}', $${service});!!{cursor}"
  },
  {
    "name": "Factory",
    "trigger": "ngfac",
    "usage": "ng factoryName",
    "description": "AngularJS factory",
    "template": ".factory('$${factory}', $${factory});!!{cursor}"
  },
  {
    "name": "Directive",
    "trigger": "ngdir",
    "usage": "ng direciveName",
    "description": "AngularJS directive",
    "template": ".directive('$${directive}', $${directive});!!{cursor}"
  },
  {
    "name": "Function for Angular service",
    "trigger": "ngf",
    "usage": "ng functionName",
    "description": "Create a function for AngularJS service (controller, directive)",
    "template": "var $${name} = function() {\n!!{cursor}\n}"
  },
  {
